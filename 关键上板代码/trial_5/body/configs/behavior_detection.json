{
  "inputs": [
    "image"
  ],
  "outputs": [
    "image",
    "face_final_box",
    "head_final_box",
    "body_final_box",
    "disappeared_track_id",
    "pose",
    "lmk",
    "kps",
    "raisehand",
    "detect"
  ],
  "workflow": [
    {
      "thread_count": 3,
      "method_type": "FasterRCNNMethod",
      "unique_name": "multi_task",
      "inputs": [
        "image"
      ],
      "outputs": [
        "body_box",
        "head_box",
        "face_box",
        "lmk",
        "pose",
        "kps"
      ],
      "method_config_file": "multitask_config.json"
    },
    {
      "thread_count": 1,
      "method_type": "MOTMethod",
      "unique_name": "face_mot",
      "inputs": [
        "face_box"
      ],
      "outputs": [
        "face_bbox_list",
        "face_disappeared_track_id_list"
      ],
      "method_config_file": "iou_method_param.json"
    },
    {
      "thread_count": 1,
      "method_type": "MOTMethod",
      "unique_name": "head_mot",
      "inputs": [
        "head_box"
      ],
      "outputs": [
        "head_bbox_list",
        "head_disappeared_track_id_list"
      ],
      "method_config_file": "iou_method_param.json"
    },
    {
      "thread_count": 1,
      "method_type": "MOTMethod",
      "unique_name": "body_mot",
      "inputs": [
        "body_box"
      ],
      "outputs": [
        "body_bbox_list",
        "body_disappeared_track_id_list"
      ],
      "method_config_file": "iou_method_param.json"
    },
    {
      "thread_count": 1,
      "method_type": "MergeMethod",
      "unique_name": "merge_method",
      "inputs": [
        "face_bbox_list",
        "head_bbox_list",
        "body_bbox_list",
        "face_disappeared_track_id_list",
        "head_disappeared_track_id_list",
        "body_disappeared_track_id_list",
        "kps"
      ],
      "outputs": [
        "face_final_box",
        "head_final_box",
        "body_final_box",
        "disappeared_track_id"
      ],
      "method_config_file": "merge_head_body_behavior.json"
    },
    {
      "method_type": "BehaviorMethod",
      "unique_name": "behavior_raisehand",
      "inputs": [
        "body_final_box",
        "disappeared_track_id",
        "kps"
      ],
      "outputs": [
        "raisehand"
      ],
      "method_config_file": "raisehand_behavior.json"
    },
    {
      "method_type": "BehaviorMethod",
      "unique_name": "behavior_detect",
      "inputs": [
        "body_final_box",
        "disappeared_track_id",
        "kps"
      ],
      "outputs": [
        "detect"
      ],
      "method_config_file": "detect_behavior.json"
    }
  ]
}
